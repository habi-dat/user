mixin header(title, titleMessage)
  doctype transitional
  head
    meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
    title=title
  center
    table(width='600', background='#FFFFFF', style='text-align:left;', cellpadding='0', cellspacing='0')
      // GREEN STRIPE
      tr
        td( width='31', bgcolor='#45a853', style="border-top:1px solid #FFF; border-bottom:1px solid #FFF;background-image: url('data:image/gif;base64,R0lGODlhAQBxAIACAKQAI8cIMSH+EUNyZWF0ZWQgd2l0aCBHSU1QACwAAAAAAQBxAAACDYSPqcvtHaKctNqLZwEAOw==');", height='113')
          div(style='line-height: 0px; font-size: 1px; position: absolute;') &nbsp;
        // WHITE TEXT AREA
        td(width='131', bgcolor='#FFFFFF', style='border-top:1px solid #FFF; text-align:center;', height='113', valign='middle')
          span(style='font-size:25px; font-family:Trebuchet MS, Verdana, Arial; color:#2e8a3b;')
            img(src="data:image/png;base64,")
        // GREEN TEXT AREA
        td(background='images/greenback.gif', bgcolor='#45a853', style="border-top:1px solid #FFF; border-bottom:1px solid #FFF; padding-left:15px;background-image: url('data:image/gif;base64,R0lGODlhAQBxAIACAKQAI8cIMSH+EUNyZWF0ZWQgd2l0aCBHSU1QACwAAAAAAQBxAAACDYSPqcvtHaKctNqLZwEAOw==');", height='113')
          span(style='color:#FFFFFF; font-size:18px; font-family:Trebuchet MS, Verdana, Arial;')=titleMessage
      // DOUBLE BORDERS BOTTOM
      tr
        td(height='3', width='31', style='border-top:1px solid #e4e4e4; border-bottom:1px solid #e4e4e4;')
          div(style='line-height: 0px; font-size: 1px; position: absolute;') &nbsp;
        td(height='3', width='131')
          div(style='line-height: 0px; font-size: 1px; position: absolute;') &nbsp;
        td(height='3', style='border-top:1px solid #e4e4e4; border-bottom:1px solid #e4e4e4;')
          div(style='line-height: 0px; font-size: 1px; position: absolute;') &nbsp;
      tr
        td(colspan='3')
          block 
    br
    table(cellpadding='0', style='border-top:1px solid #e4e4e4; text-align:center; font-family:Trebuchet MS, Verdana, Arial; font-size:12px;', cellspacing='0', width='600')
      tr
        td(height='2', style='border-bottom:1px solid #e4e4e4;')
          div(style='line-height: 0px; font-size: 1px; position: absolute;') &nbsp;
      td(style='font-family:Trebuchet MS, Verdana, Arial; font-size:12px;')
        br
        p 
          span Diese E-Mail wurde über #{config.settings.general.subdomain}.#{config.settings.general.domain} verschickt. Falls hier ein Fehler passiert ist, bitten wir dich uns Bescheid zu geben: 
          span
            a(href='mailto:'+config.settings.general.contact) #{config.settings.general.contact}

mixin links
  // RIGHT COLUMN FIRST BOX
  if config.settings.general.modules && config.settings.general.modules.includes('nextcloud')
    table(width='100%', cellpadding='0', cellspacing='0', style='border-bottom:1px solid #e4e4e4; font-family:Trebuchet MS, Verdana, Arial; font-size:12px;')
      tr
        td
          div(style='font-family:Trebuchet MS, Verdana, Arial; font-size:17px; font-weight:bold; padding-bottom:10px;') Nextcloud: Dateien, Kalender und mehr
          img(src='data:image/png;base64,', align='right', style='padding-left:10px; padding-top:10px; padding-bottom:10px;', alt='')
          p Die Nextcloud ist der Einstiegspunkt, hier findest du Dateien, Kalender, Kontakte und die Links zu allen Apps: 
            a(href='https://' + config.nextcloud.subdomain + '.' + config.settings.general.domain style='text-decoration: none;color:black;')
              p
                b= config.nextcloud.subdomain + '.' + config.settings.general.domain
          br
  // RIGHT COLUMN SECOND BOX
  if config.settings.general.modules && config.settings.general.modules.includes('discourse')
    br
    table(width='100%', cellpadding='0', cellspacing='0', style='border-bottom:1px solid #e4e4e4; font-family:Trebuchet MS, Verdana, Arial; font-size:12px;')
      tr
        td
          div(style='font-family:Trebuchet MS, Verdana, Arial; font-size:17px; font-weight:bold; padding-bottom:10px;') Discourse: Forum und Messenger
          img(src='data:image/png;base64,', align='right', style='padding-left:10px; padding-top:10px; padding-bottom:10px;', alt='')
          p Discourse dient als Forum und Messenger. Du kannst es über die Nextcloud erreichen oder direkt hierrüber:
            a(href='https://' + config.discourse.subdomain + '.' + config.settings.general.domain style='text-decoration: none;color:black;')
              p
                b= config.discourse.subdomain + '.' + config.settings.general.domain
          br
  // RIGHT COLUMN THIRD BOX
  if config.settings.general.modules && config.settings.general.modules.includes('dokuwiki')
    br
    table(width='100%', cellpadding='0', cellspacing='0', style='border-bottom:1px solid #e4e4e4; font-family:Trebuchet MS, Verdana, Arial; font-size:12px;')
      tr
        td
          div(style='font-family:Trebuchet MS, Verdana, Arial; font-size:17px; font-weight:bold; padding-bottom:10px;') Dokuwiki: Wissensspeicher
          img(src='data:image/png;base64,', align='right', style='padding-left:10px; padding-top:10px; padding-bottom:10px;', alt='')
          p Mit Dokuwiki kannst du Abläufe in deinem Projekt dokumentieren. Du kannst es über die Nextcloud erreichen oder direkt hier:
            a(href='https://' + config.dokuwiki.subdomain + '.' + config.settings.general.domain style='text-decoration: none;color:black;')
              p
                b= config.dokuwiki.subdomain + '.' + config.settings.general.domain
          br
  // RIGHT COLUMN THIRD BOX
  if config.settings.general.modules && config.settings.general.modules.includes('mediawiki')
    br
    table(width='100%', cellpadding='0', cellspacing='0', style='border-bottom:1px solid #e4e4e4; font-family:Trebuchet MS, Verdana, Arial; font-size:12px;')
      tr
        td
          div(style='font-family:Trebuchet MS, Verdana, Arial; font-size:17px; font-weight:bold; padding-bottom:10px;') Mediawiki: Organize!
          img(src='data:image/png;base64,', align='right', style='padding-left:10px; padding-top:10px; padding-bottom:10px;', alt='')
          p Mit Medaiwiki (inkl Semantic Organisation) kannst du Teams, Aufgaben, Projekte und die Protokolle eurer Treffen verwalten:
            a(href='https://' + config.mediawiki.subdomain + '.' + config.settings.general.domain style='text-decoration: none;color:black;')
              p
                b= config.mediawiki.subdomain + '.' + config.settings.general.domain
          br
